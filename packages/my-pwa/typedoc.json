{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "exclude": ["src/test/**/*", "**/*.test.ts", "**/*.test.tsx"],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "includeVersion": true,
  "theme": "default",
  "name": "@btc-wallet/my-pwa",
  "description": "Comprehensive PWA development toolkit for Bitcoin wallet applications",
  "readme": "README.md",
  "categorizeByGroup": true,
  "categoryOrder": [
    "PWA Core",
    "PWA Components",
    "PWA Hooks",
    "PWA Utilities",
    "PWA Styles",
    "PWA Types",
    "*"
  ],
  "defaultCategory": "Other",
  "plugin": ["typedoc-plugin-markdown"],
  "mdHideSources": true,
  "mdSourceRepo": "https://github.com/btc-wallet/my-pwa",
  "mdSourceBranch": "main",
  "mdSourceBasePath": "packages/my-pwa/src"
}
